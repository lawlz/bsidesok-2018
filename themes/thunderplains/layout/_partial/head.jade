meta(charset='utf-8')
meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1')
meta(content="BSides OK Security Conference", name="description")

meta(name="twitter:card", content="summary")
meta(name="twitter:site", content="@BSidesOK")
meta(name="twitter:title", content="BSidesOK Security Conference")
meta(name="twitter:description" content="BSidesOK is a free information security conference in Tulsa, OK organized by local volunteers.")
meta(name="twitter:image" content="http://bsidesok.com/images/bsides-2.png")

link(href="favicon.ico", rel="shortcut icon")
link(href="images/apple-touch-icon-57x57.png", rel="apple-touch-icon-precomposed")
link(href="images/apple-touch-icon-72x72.png", rel="apple-touch-icon-precomposed", sizes="72x72")
link(href="images/apple-touch-icon-114x144.png", rel="apple-touch-icon-precomposed", sizes="114x114")

if config.author
  meta(name='author', content=config.author)

block page_description
  meta(name='description', content=config.description)

title
  block window_title
    = config.title

link(href=config.root + 'favicon.png', rel='icon')

link(rel='alternate', href=theme.rss ? theme.rss : config.root + 'atom.xml', title='config.title', type='application/atom.xml')

link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Open+Sans|Raleway' type='text/css')
link(href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css", rel="stylesheet", integrity="sha384-T8Gy5hrqNKT+hzMclPo118YTQO6cYprQmhrYwIiQ/3axmI1hQomh7Ud2hPOy8SP1", crossorigin="anonymous")
link(rel='stylesheet', href=config.root + 'stylesheets/thunderplains.css')


script(src="http://code.jquery.com/jquery-1.10.1.min.js")
script(src="javascripts/bootstrap-modal.js", type="text/javascript")
script(src="javascripts/bootstrap-tabpane.js", type="text/javascript")
script(src="javascripts/application.js", type="text/javascript")
